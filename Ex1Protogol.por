programa {
inteiro opcao = 1




  funcao inicio() {
    escreva ("Digite a opção")
    leia(opcao)

    escolha (opcao){
    caso 1:
    escreva("Está no caso 1")
    caso 2 :
    escreva("Está no caso 2")
    caso 3 :
    escreva("Está no caso 3")
    
    outrocaso :
    escreva("Sem codigo")}
  }
}
